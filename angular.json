{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "srm": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "less",
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/srm",
      "sourceRoot": "projects/srm/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/srm/browser",
            "index": "projects/srm/src/index.html",
            "main": "projects/srm/src/main.ts",
            "polyfills": "projects/srm/src/polyfills.ts",
            "tsConfig": "projects/srm/tsconfig.app.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "projects/srm/src/favicon.svg",
              "projects/srm/src/assets"
            ],
            "styles": [
              "projects/srm/src/styles.less"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kb",
                  "maximumError": "8kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/srm/src/environments/environment.ts",
                  "with": "projects/srm/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "staging": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kb",
                  "maximumError": "8kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/srm/src/environments/environment.ts",
                  "with": "projects/srm/src/environments/environment.staging.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "srm:build:production"
            },
            "development": {
              "browserTarget": "srm:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "srm:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/srm/src/test.ts",
            "polyfills": "projects/srm/src/polyfills.ts",
            "tsConfig": "projects/srm/tsconfig.spec.json",
            "karmaConfig": "projects/srm/karma.conf.js",
            "inlineStyleLanguage": "less",
            "assets": [
              "projects/srm/src/favicon.svg",
              "projects/srm/src/assets"
            ],
            "styles": [
              "projects/srm/src/styles.less"
            ],
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/srm/server",
            "main": "projects/srm/server.ts",
            "tsConfig": "projects/srm/tsconfig.server.json",
            "inlineStyleLanguage": "less"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "projects/srm/src/environments/environment.ts",
                  "with": "projects/srm/src/environments/environment.prod.ts"
                }
              ]
            },
            "staging": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "projects/srm/src/environments/environment.ts",
                  "with": "projects/srm/src/environments/environment.staging.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "srm:build:development",
              "serverTarget": "srm:server:development"
            },
            "production": {
              "browserTarget": "srm:build:production",
              "serverTarget": "srm:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {
              "browserTarget": "srm:build:production",
              "serverTarget": "srm:server:production"
            },
            "staging": {
              "browserTarget": "srm:build:staging",
              "serverTarget": "srm:server:staging"
            },
            "development": {
              "browserTarget": "srm:build:development",
              "serverTarget": "srm:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "defaultProject": "srm"
}
