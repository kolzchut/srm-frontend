    "@angular/platform-server": "^17.2.1",
    "@angular/router": "^17.2.1",
    "@angular/ssr": "^17.2.0",
    "@ngneat/until-destroy": "^9.2.2",
    "@sentry/angular-ivy": "^7.80.1",
